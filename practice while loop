#just practice
fardin = 1
fardin2 = []
while fardin <= 40:
    if fardin % 4 == 0:
        fardin += 1
        print("skiped")
        continue
    elif fardin %3 == 0 and fardin % 5 != 0:
        print("bonus",fardin)
    elif fardin == 25:
        break
    else:
        print("add",fardin)
    fardin2.append(fardin)
    fardin += 1
print("total steps",fardin2)
print(type(fardin2))
# new 
#password checker
password = 1234
write = []
wrong = []
attempt = 0
while True:
    user = int(input("write password: "))
    if password == user:
        print("password is correct")
        write.append(user)
        break
    else:
        print("wrong password")
        attempt += 1
        wrong.append(user)
    if attempt == 5:
        print("too many attempts")
        break

print(len(write) , "your total write attempt")
print(len(wrong),"your total wrong attempt")












